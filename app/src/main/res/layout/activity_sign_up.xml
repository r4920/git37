<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpVM" type="com.rahulsapplication.app.modules.signup.data.viewmodel.SignUpVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSignUp"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblack_900">
        <LinearLayout android:id="@+id/linearGroup31"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_22pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imageIconArrowLe"
                android:layout_gravity="start"
                android:layout_height="@dimen/_24pxh"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY" android:src="@drawable/img_iconarrowle"/>
            <TextView android:gravity="start"
                android:id="@+id/txtSignUpAndsta"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="match_parent"
                android:text="@{signUpVM.signUpModel.txtSignUpAndsta}"
                style="@style/txtRobotobold35" tools:text="@string/msg_sign_up_and_sta"/>
            <TextView android:gravity="start"
                android:id="@+id/txtYourName"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_46pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_21pxv"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_19pxv"
                android:text="@{signUpVM.signUpModel.txtYourName}"
                style="@style/txtSolidRounded" tools:text="@string/lbl_your_name"/>
            <TextView android:gravity="start"
                android:id="@+id/txtYourName1"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_21pxv"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_19pxv"
                android:text="@{signUpVM.signUpModel.txtYourName1}"
                style="@style/txtSolidRounded" tools:text="@string/lbl_your_name"/>
            <TextView android:gravity="start"
                android:id="@+id/txtYourName2"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_21pxv"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_19pxv"
                android:text="@{signUpVM.signUpModel.txtYourName2}"
                style="@style/txtSolidRounded" tools:text="@string/lbl_your_name"/>
            <TextView android:gravity="start"
                android:id="@+id/txtYourName3"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_21pxv"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_19pxv"
                android:text="@{signUpVM.signUpModel.txtYourName3}"
                style="@style/txtSolidRounded" tools:text="@string/lbl_your_name"/>
            <TextView android:alpha="0.5" android:gravity="start"
                android:id="@+id/txtBySigningUpY"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_31pxh"
                android:layout_marginStart="@dimen/_31pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent"
                android:text="@{signUpVM.signUpModel.txtBySigningUpY}"
                style="@style/txtInterregular12" tools:text="@string/msg_by_signing_up_y"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnSignUpNow"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_94pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_sign_up_now"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_2" tools:text="@string/lbl_sign_up_now"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAlreadyRegiste"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_43pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signUpVM.signUpModel.txtAlreadyRegiste}"
                style="@style/txtRobotoregular12" tools:text="@string/msg_already_registe"/>
        </LinearLayout>
    </LinearLayout>
</layout>
